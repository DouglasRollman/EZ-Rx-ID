   "source": [
    "#THIS ONE USES TRAIN/TEST SPLIT 80%-20%\n",
    "import torch  #for building the neural network\n",
    "import torch.nn as nn  #neural network module\n",
    "import torch.optim as optim  #optimizers for training\n",
    "import time  \n",
    "import pandas as pd  \n",
    "import numpy as np  \n",
    "from sklearn.preprocessing import LabelEncoder  #label encoder for categorical data\n",
    "from sklearn.model_selection import train_test_split  #train-test split function\n",
    "from torch.utils.data import DataLoader, TensorDataset  #dataloader for batching data\n",
    "from sklearn.preprocessing import MinMaxScaler  #scaler to normalize numerical data\n",
    "from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score  #evaluation metrics\n",
    "\n",
    "#loading  dataset\n",
    "file_path = \"df_rximages_final.csv\"  #dataset file path\n",
    "df = pd.read_csv(file_path)  #read the dataset into a df\n",
    "\n",
    "#remove missing vals\n",
    "df.dropna(inplace=True)  #drop rows with missing vals"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "aebb0a90-85d6-4b71-bdc8-754c3d309615",
   "metadata": {},
   "outputs": [],
   "source": [
    "#encode categorical features\n",
    "label_encoders = {}  #dictionary to store encoders\n",
    "for column in [\"shape\", \"color\", \"imprint\", \"name\"]:  #loop through categorical cols\n",
    "    le = LabelEncoder()  #initialize label encoder\n",
    "    df[column] = le.fit_transform(df[column])  #convert text to nums\n",
    "    label_encoders[column] = le  #store encoder for later use"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c1dbf227-cacf-4c3b-9503-9cc685d96733",
   "metadata": {},
   "outputs": [],
   "source": [
    "#define features and target var\n",
    "X = df[[\"shape\", \"color\", \"size\", \"imprint\"]]  #select input features\n",
    "y = df[\"name\"]  #target var"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ecacb542-76d7-4f46-a7d5-4e75f858093e",
   "metadata": {},
   "outputs": [],
   "source": [
    "#normalize numerical data\n",
    "scaler = MinMaxScaler()  #initialize min-max scaler\n",
    "X_scaled = scaler.fit_transform(X)  #scale features between 0 and 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "22a94c2f-8f13-48f2-ba41-8445dae038b3",
   "metadata": {},
   "outputs": [],
   "source": [
    "#split data into train and test sets\n",
    "X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)  #80% train, 20% test"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "13f9f01e-f754-4b62-a3e5-925e060733ac",
   "metadata": {},
   "outputs": [],
   "source": [
    "#convert data to pytorch tensors\n",
    "X_train_tensor = torch.tensor(X_train, dtype=torch.float32)  #convert X_train to tensor\n",
    "y_train_tensor = torch.tensor(y_train.values, dtype=torch.long)  #convert y_train to tensor\n",
    "X_test_tensor = torch.tensor(X_test, dtype=torch.float32)  #convert X_test to tensor\n",
    "y_test_tensor = torch.tensor(y_test.values, dtype=torch.long)  #convert y_test to tensor"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "851c3706-4bf1-40cf-8280-e2ffb248b5a1",
   "metadata": {},
   "outputs": [],
   "source": [
    "#create dataloaders for training and testing\n",
    "train_loader = DataLoader(TensorDataset(X_train_tensor, y_train_tensor), batch_size=8, shuffle=False)  #load training data\n",
    "test_loader = DataLoader(TensorDataset(X_test_tensor, y_test_tensor), batch_size=8, shuffle=False)  #load test data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "18c2e044-c04d-43b8-927b-b68744da13bf",
   "metadata": {},
   "outputs": [],
   "source": [
    "#define neural network model\n",
    "class OverfitNN(nn.Module):  #defines model class\n",
    "    def __init__(self):\n",
    "        super(OverfitNN, self).__init__()  #initializes parent class\n",
    "        self.model = nn.Sequential(\n",
    "            nn.Linear(4, 1024),  #input layer with 4 features and 1024 neurons\n",
    "            nn.ReLU(),  #activation function to introduce non-linearity\n",
    "            nn.Linear(1024, 1024),  #hidden layer with 1024 neurons\n",
    "            nn.ReLU(),  #activation function\n",
    "            nn.Linear(1024, 1024),  #another hidden layer\n",
    "            nn.ReLU(),  #activation function\n",
    "            nn.Linear(1024, len(np.unique(y)))  #output layer with num of pill names\n",
    "        )\n",
    "\n",
    "    def forward(self, x):  #forward pass function\n",
    "        return self.model(x)  #pass input through the network\n",
    "\n",
    "#initialize the model\n",
    "model = OverfitNN()  #create model instance"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "33659846-a142-4091-9425-a0a0bd89806d",
   "metadata": {},
   "outputs": [],
   "source": [
    "#loss function and optimizer\n",
    "criterion = nn.CrossEntropyLoss()  #cross entropy loss for classification\n",
    "optimizer = optim.Adam(model.parameters(), lr=0.001, weight_decay=0)  #adam optimizer, no weight decay"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "e797468c-3b73-46ba-b173-e7061e8c439b",
   "metadata": {},
   "outputs": [],
   "source": [
    "#training loop\n",
    "num_epochs = 1000  #number of epochs\n",
    "start_time = time.time()  #track start time\n",
    "\n",
    "for epoch in range(1, num_epochs + 1):  #loop through epochs\n",
    "    model.train()  #set model to training mode\n",
    "    running_loss = 0.0  #track loss\n",
    "    correct_predictions = 0  #track correct predictions\n",
    "    total_predictions = 0  #track total predictions\n",
    "    \n",
    "    for inputs, labels in train_loader:  #loop through batches\n",
    "        optimizer.zero_grad()  #reset gradients\n",
    "        outputs = model(inputs)  #forward pass\n",
    "        loss = criterion(outputs, labels)  #compute loss\n",
    "        loss.backward()  #backpropagation\n",
    "        optimizer.step()  #update weights\n",
    "        \n",
    "        running_loss += loss.item()  #accumulate loss\n",
    "        \n",
    "        _, predicted = torch.max(outputs, 1)  #get predicted class\n",
    "        correct_predictions += (predicted == labels).sum().item()  #count correct predictions\n",
    "        total_predictions += labels.size(0)  #count total predictions\n",
    "    \n",
    "    train_accuracy = (correct_predictions / total_predictions) * 100  #calculate accuracy\n",
    "    print(f\"Epoch [{epoch}/{num_epochs}], Loss: {running_loss/len(train_loader):.4f}, Train Accuracy: {train_accuracy:.2f}%\")  #print progress\n",
    "\n",
    "#training complete\n",
    "training_time = time.time() - start_time  #calculate total training time\n",
    "print(f\"Training time: {training_time:.4f} seconds\")  #print training time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "7aff3510-e695-4d4e-ba02-94a6db3b4e15",
   "metadata": {},
   "outputs": [],
   "source": [
    "#testing loop\n",
    "model.eval()  #set model to evaluation mode\n",
    "correct_predictions = 0  #reset correct predictions\n",
    "total_predictions = 0  #reset total predictions\n",
    "all_predictions = []  #store all predictions\n",
    "all_labels = []  #store actual labels\n",
    "\n",
    "with torch.no_grad():  #disable gradient tracking\n",
    "    for inputs, labels in test_loader:  #loop through test data\n",
    "        outputs = model(inputs)  #forward pass\n",
    "        _, predicted = torch.max(outputs, 1)  #get predicted class\n",
    "        correct_predictions += (predicted == labels).sum().item()  #count correct predictions\n",
    "        total_predictions += labels.size(0)  #count total predictions\n",
    "        \n",
    "        all_predictions.extend(predicted.cpu().numpy())  #store predictions\n",
    "        all_labels.extend(labels.cpu().numpy())  #store actual labels"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ac7614eb-bb8b-45a8-b119-768449e71349",
   "metadata": {},
   "outputs": [],
   "source": [
    "#function for predicting pillname\n",
    "def predict_pill(shape, color, size, imprint):  #function to predict pill name\n",
    "    shape_encoded = label_encoders[\"shape\"].transform([shape])[0]  #encode shape\n",
    "    color_encoded = label_encoders[\"color\"].transform([color])[0]  #encode color\n",
    "    imprint_encoded = label_encoders[\"imprint\"].transform([imprint])[0]  #encode imprint\n",
    "    \n",
    "    #convert input to df\n",
    "    input_data = pd.DataFrame([[shape_encoded, color_encoded, size, imprint_encoded]], columns=X.columns)\n",
    "    \n",
    "    #scale input data\n",
    "    input_scaled = scaler.transform(input_data)  #normalize input\n",
    "    \n",
    "    #convert to tensor\n",
    "    input_tensor = torch.tensor(input_scaled, dtype=torch.float32)  #convert input to tensor\n",
    "    \n",
    "    #make prediction\n",
    "    with torch.no_grad():  #disable gradients\n",
    "        output = model(input_tensor)  #forward pass\n",
    "        _, prediction = torch.max(output, 1)  #get predicted class\n",
    "        predicted_pill = label_encoders[\"name\"].inverse_transform(prediction)  #convert back to pill name\n",
    "    \n",
    "    return predicted_pill[0]  #return predicted pill name"
   ]
  },
