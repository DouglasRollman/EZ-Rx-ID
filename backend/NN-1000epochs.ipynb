    "from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score\n",
    "\n",
    "#store true labels and model predictions\n",
    "all_predictions = []\n",
    "all_labels = []\n",
    "\n",
    "#switch model to eval mode to disable training behaviors like dropout\n",
    "model.eval()\n",
    "\n",
    "with torch.no_grad():  #disable gradient tracking to save memory\n",
    "    for inputs, labels in train_loader:  \n",
    "        outputs = model(inputs)\n",
    "        _, predicted = torch.max(outputs, 1)  #get index of highest prob (predicted class)\n",
    "        all_predictions.extend(predicted.cpu().numpy())  #store predictions\n",
    "        all_labels.extend(labels.cpu().numpy())  #store actual labels\n",
    "\n",
    "#calculate accuracy, precision, recall, and f1-score\n",
    "accuracy = accuracy_score(all_labels, all_predictions)  #measure overall correctness\n",
    "precision = precision_score(all_labels, all_predictions, average='weighted', zero_division=1)  #measure how many predicted pills were correct\n",
    "recall = recall_score(all_labels, all_predictions, average='weighted', zero_division=1)  #measure how well the model found all pills correctly\n",
    "f1 = f1_score(all_labels, all_predictions, average='weighted', zero_division=1)  #balance between precision & recall\n",
    "\n",
    "#print scores\n",
    "print(f\"Accuracy: {accuracy:.4f}\")\n",
    "print(f\"Precision: {precision:.4f}\")\n",
    "print(f\"Recall: {recall:.4f}\")\n",
    "print(f\"F1 Score: {f1:.4f}\")\n"
   ]
